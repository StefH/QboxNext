{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet",
    "FUNCTIONS_EXTENSION_VERSION": "~3",

    // Change this to a different value in Azure , like "0 0 * * *" ==> “At 01:00”
    "CleanupQboxStatesCronExpression": "* * * * *",

    // Change this to a different value in Azure , like "0 3 * * *" ==> “At 03:00”
    "CleanupQboxLoggingCronExpression": "* * * * *"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AzureTableStorageCleanerOptions": {
    "ConnectionString": "UseDevelopmentStorage=true",

    "LoggingTableName": "QboxLogging",
    "LoggingTableRetentionInMonths": 11,
    "LoggingTableDeleteRows": false,
    "LoggingTableCronExpressionNOTUSED": "0 3 * * *",

    "StatesTableName": "QboxStates",
    "StatesTableRetentionInMonths": 11,
    "StatesTableDeleteRows": false,
    "StatesTableCronExpressionNOTUSED": "0 0 * * *"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "<replace>"
  }
}