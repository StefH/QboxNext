<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>1.0.0.0</VersionPrefix>
    <Description>QboxNext is a modified clone of Qplatform.</Description>
    <AssemblyTitle>QboxNext</AssemblyTitle>
    <Authors>Sebastiaan;Stef Heyenrath</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <!--<GenerateDocumentationFile>true</GenerateDocumentationFile>-->
    <PackageId>QboxNext</PackageId>
    <PackageTags>qbox;qboxnext;qurrent;server</PackageTags>
    <PackageReleaseNotes>See CHANGELOG.md</PackageReleaseNotes>
    <!--<PackageIconUrl>https://raw.githubusercontent.com/WireMock-Net/WireMock.Net/master/WireMock.Net-Logo.png</PackageIconUrl>-->
    <PackageProjectUrl>https://github.com/stefh/QboxNext</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/stefh/QboxNext/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/stefh/QboxNext</RepositoryUrl>
    <!--<ApplicationIcon>../../WireMock.Net-Logo.ico</ApplicationIcon>-->
    <DebugType>full</DebugType>
    <IncludeSource>True</IncludeSource>
    <IncludeSymbols>True</IncludeSymbols>
    <ProjectGuid>{D3804229-91F4-4502-9595-39884E5A01AD}</ProjectGuid>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <!--<CodeAnalysisRuleSet>WireMock.Net.ruleset</CodeAnalysisRuleSet>-->
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <PathMap>$(MSBuildProjectDirectory)=/</PathMap>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="NLog" Version="4.5.11" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Exclude="..\..\dependencies\dotnetcore-minimal\Qboxes.Model\obj\**\*.cs" Include="..\..\dependencies\dotnetcore-minimal\Qboxes.Model\**\*.cs" />
    <Compile Exclude="..\..\dependencies\dotnetcore-minimal\QboxNext.Core\obj\**\*.cs" Include="..\..\dependencies\dotnetcore-minimal\QboxNext.Core\**\*.cs" />
    <Compile Exclude="..\..\dependencies\dotnetcore-minimal\QboxNext.Qboxes.Parsing\obj\**\*.cs" Include="..\..\dependencies\dotnetcore-minimal\QboxNext.Qboxes.Parsing\**\*.cs" />
    <Compile Exclude="..\..\dependencies\dotnetcore-minimal\QboxNext.Qserver.Core\obj\**\*.cs" Include="..\..\dependencies\dotnetcore-minimal\QboxNext.Qserver.Core\**\*.cs" />
  </ItemGroup>

  <ItemGroup>
