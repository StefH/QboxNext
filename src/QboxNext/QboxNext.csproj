    <Compile Include="..\..\dependencies\dotnetcore-minimal\QboxNext.Qboxes.Parsing\obj\$(Configuration)\netcoreapp2.1\NuGet\97D72B49C162339BCD9CBFDE1E2259197AFE9F35\LibLog\5.0.4\*.cs" Link="Logging.QboxNext.Qboxes.Parsing\*.cs" />
